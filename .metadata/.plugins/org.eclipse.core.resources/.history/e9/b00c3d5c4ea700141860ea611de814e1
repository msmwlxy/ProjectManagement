package org.future.projectManagement.service;

import org.future.projectManagement.bean.Student;
import org.future.projectManagement.dao.StudentDao;

public class StudentService {
	
	private StudentDao studentDao;

	public StudentDao getStudentDao() {
		return studentDao;
	}

	public void setStudentDao(StudentDao studentDao) {
		this.studentDao = studentDao;
	}
	
	public void add(Student student){
		studentDao.add(student);
	}
	
	public Student find(Class student,String id){
		return studentDao.find(student, id);
	}
}
