function showwin(e,str){
alert("sdfdfs");
	var x = getX(e);

	var y = getY(e);

	var win = $("#win");
	//win.css("display","block");
	//win.show("slow");
	win.css("position","absolute");
	win.css("left",x);
	win.css("top",y);
	$("#win").children("#cont").html(str);
	//win.$("#cont").html(str);
	win.fadeIn("slow");
	return false;
}
function closewin(){
	var win = $("#win");
	//win.css("display","none");
	//win.hide("slow");
	win.fadeOut("slow");
	
}
$(document).ready(function(){
$("#aa").mousemove(function(e) {  

	var xx=e.pageX;
	var yy=e.pageY;
	
}); 

});
function getX(e) {
  e = e || window.event;
    
return e.pageX || e.clientX + document.body.scroolLeft;
}
function getY(e) {
  e = e|| window.event;
 return e.pageY || e.clientY + document.boyd.scrollTop;
} 