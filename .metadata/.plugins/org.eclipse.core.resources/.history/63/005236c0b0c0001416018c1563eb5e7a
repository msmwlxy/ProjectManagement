package org.future.projectManagement.utils;

public class WhereBuild {
	public String whereBuild(String[] wheres){
		String where = null;
		if(wheres.length == 0){
			return null;
		}else{
			for(int i = 0; i < wheres.length; i ++){
				if(i == wheres.length){
					where += "o."+ wheres[i] + "? ";					
				}
			}
			return where;
		}
	}
}
