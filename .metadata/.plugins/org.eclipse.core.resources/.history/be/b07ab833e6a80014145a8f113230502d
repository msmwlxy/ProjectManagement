package org.future.projectManagement.service;

import java.util.Date;
import java.util.List;

public interface ManyConditionsQueryService {
	
	/**
	 * 方法作用：得到要查询的记录个数
	 * @param cId ：院系  
	 * @param startTime ：开始时间
	 * @param endTime ： 结束时间
	 * @param pId ：知道老师
	 * @param psId ： 状态
	 * @param outStanding ：是否优秀
	 * 以上参数都是查询条件
	 * @return ：得到要查询的记录个数
	 */
	public int getCount(int cId, Date startTime, Date endTime, int pId, int psId, Boolean outStanding);  
	  
	/**
	 * 方法作用：得到要查询的记录
	 * @param cId
	 * @param startTime
	 * @param endTime
	 * @param pId
	 * @param psId
	 * @param outStanding
	 * @param maxRow
	 * @param currentPage
	 * @return
	 */
	public List<?> search(int cId, Date startTime, Date endTime, int pId, int psId, Boolean outStanding, int maxRow, int currentPage); 
}
